footer
  .container
    .col-md-12.col-sm-12
      .f-about-col
        p
          = t('footer.copyright')
        p
          = t('brand')
          | .
    .col-md-6.col-sm-6
      .flink
        h2
          = t('footer.main')
        ul
          li
            a href="#{about_affiliate_path}"
              = t('footer.affiliate')
          li
            a href="#{fees_path}"
              = t('footer.fees')
          li
            a href="#{terms_path}"
              = t('footer.terms')
          li
            a href="#{privacy_path}"
              = t('footer.privacy')
          / li
          /   a href="#{about_path}"
          /     = t('footer.about')
          li
            a href="mailto:support@rabbit-cc.com"
              = t('footer.contact')
    .col-md-6.col-sm-6
      .flink
        h2
          = t('footer.others')
        ul
          li
            a href="https://www.facebook.com/Rabbit-Exchange-464100197418359/"
              i.fa.fa-facebook-square
              span = t('social.facebook')
          li
            a href="https://www.linkedin.com/company/rabbit-exchange/"
              i.fa.fa-linkedin-square
              span = t('social.linkedin')
          li
            a href="https://twitter.com/exchange_rabbit"
              i.fa.fa-twitter-square
              span = t('social.twitter')