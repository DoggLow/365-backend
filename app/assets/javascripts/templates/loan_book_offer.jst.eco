<tr data-id='<%= @id %>' data-rate='<%= @rate %>' data-amount='<%= @amount %>' data-duration='<%= @duration %>' class='<%= @classes %>'>
  <td class='duration col-xs-6'><div>
    <%- loan_formatter.mask_duration @duration %>
  </div></td>
  <td class='amount col-xs-10'><div>
    <%- loan_formatter.mask_fixed_amount @amount %>
  </div></td>
  <td class='rate col-xs-8 text-down'><div>
    <%- loan_formatter.mask_fixed_rate @rate %>
  </div></td>
</tr>
